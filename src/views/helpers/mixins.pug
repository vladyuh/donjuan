mixin icons(id, size, modify)
  if size
    svg(width=''+size+'' height=''+size+'' class=modify)
      use(xlink:href="/img/sprites/sprite.svg"+id)
  else
    svg(width="24" height="24" class=modify)
      use(xlink:href="/img/sprites/sprite.svg"+id)

mixin listUl(list)
  ul
    each val in list
      li= val